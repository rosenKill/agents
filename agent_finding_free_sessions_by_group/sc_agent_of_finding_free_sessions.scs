sc_agent_of_finding_free_sessions
=> nrel_main_idtf:
	[sc-агент поиска свободных пар] 
	(* <- lang_ru;; *);
	[sc-agent of finding] 
	(* <- lang_en;; *);

<- abstract_sc_agent;

=> nrel_primary_initiation_condition: 
	(sc_event_add_output_arc => question_initiated);

=> nrel_initiation_condition_and_result: 
	(..sc_agent_of_finding_free_sessions_condition => ..sc_agent_of_finding_free_sessions_result);

<= nrel_sc_agent_key_sc_elements: 
	{
	question_initiated;
	question;
	question_finding_free_sessions
	};

=> nrel_inclusion: 
	.platform_independent_realization_of_sc_agent_of_finding_free_sessions
	(*
	<- platform_independent_abstract_sc_agent;;
	<= nrel_sc_agent_program: 
		{
		agent_of_finding_free_sessions;
		proc_of_finding_free_sessions;
		proc_of_finding_add_param	
		};;
	-> sc_agent_of_finding_free_sessions_scp   
		(* <- active_sc_agent;; *);;
	*);;

..sc_agent_of_finding_free_sessions_condition
= [*
	question_finding_free_sessions _-> .._question;;
	question_initiated _-> .._question;;
	question _-> .._question;;
	.._question _-> .._parameter1;;
	.._question _-> .._parameter2;;
	.._question _-> .._parameter3;;
*];;

..sc_agent_of_finding_free_sessions_result
= [*
	question_finding_free_sessions _-> .._question;;
	question_finished _-> .._question;;
	question _-> .._question;;
	.._question _=> nrel_answer:: .._answer;;
	.._question _-> .._parameter1;;
	.._question _-> .._parameter2;;
	.._question _-> .._parameter3;;
*];;
